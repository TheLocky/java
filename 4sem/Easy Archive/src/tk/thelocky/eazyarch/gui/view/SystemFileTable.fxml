<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tk.thelocky.eazyarch.gui.view.SystemFileTableController">
   <children>
      <VBox prefHeight="200.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <ToolBar prefHeight="40.0" prefWidth="200.0">
              <items>
                  <Button fx:id="up_button" mnemonicParsing="false" onAction="#handleUpButtonClick" />
                  <Button fx:id="new_arch_button" mnemonicParsing="false" onAction="#handleNewArchiveButtonClick" />
              </items>
            </ToolBar>
            <TableView fx:id="fileTable" onMouseClicked="#handleLineDoubleClick" prefHeight="400.0" prefWidth="600.0">
              <columns>
                <TableColumn fx:id="fileName" prefWidth="175.0" text="Имя" />
                <TableColumn fx:id="fileType" minWidth="2.0" prefWidth="134.0" text="Тип" />
                  <TableColumn fx:id="fileSize" prefWidth="122.0" text="Размер" />
              </columns>
            </TableView>
         </children>
      </VBox>
   </children>
</AnchorPane>
